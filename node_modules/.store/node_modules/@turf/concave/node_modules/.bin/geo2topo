#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/F:/vue3练习项目/GeojsonWkt/node_modules/.store/topojson-server@3.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/F:/vue3练习项目/GeojsonWkt/node_modules/.store/topojson-server@3.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../topojson-server@3.0.1/node_modules/topojson-server/bin/geo2topo" "$@"
else
  exec node  "$basedir/../../../../../../topojson-server@3.0.1/node_modules/topojson-server/bin/geo2topo" "$@"
fi
